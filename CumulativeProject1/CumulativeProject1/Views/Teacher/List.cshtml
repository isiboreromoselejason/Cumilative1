
@model IEnumerable<CumulativeProject1.Models.Teacher>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>This page should be showing information about all Teachers</p>

<div id="containerlist">
    <h2>List of Teacher</h2>
    @foreach (var teacher in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Author/Show/@teacher.teacherId">@teacher.teacherFname @teacher.teacherLname</a></div>
        </div>
    }
</div>